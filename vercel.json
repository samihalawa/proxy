{
  "version": 2,
  "builds": [
    { "src": "api/cacheImages.js", "use": "@vercel/node" }
  ],
  "rewrites": [
    { "source": "/proxy/:url*", "destination": "https://:url*" },
    { "source": "/httpproxy/:url*", "destination": "http://:url*" },
    { "source": "/proxy-images", "destination": "/api/cacheImages" }
  ]
}
